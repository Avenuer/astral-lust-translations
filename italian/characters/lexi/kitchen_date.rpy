# game/characters/lexi/kitchen_date.rpy:2459
translate italian lexi_kitchen_date_bath_0d555218:

    # me "He abandoned her, pregnant. She committed infanticide, for which she was arrested and executed."
    me "L'ha abbandonata, incinta. Ha commesso infanticidio, per il quale è stata arrestata ed eseguita." # TL # Victorius - AI

# game/characters/lexi/kitchen_date.rpy:2460
