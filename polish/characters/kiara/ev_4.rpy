# game/characters/kiara/ev_4.rpy:73
translate polish kiara_4_1f877649:

    # me "What do you mean?"
    me "Co masz na myśli?{#mtl}{#tl}"

# game/characters/kiara/ev_4.rpy:74
translate polish kiara_4_320ed5b6:

    # kiara "Everything originates from it, as so it's a part of everything."
    kiara "Wszystko z niej pochodzi, ponieważ jest częścią wszystkiego.{#mtl}{#tl}"

# game/characters/kiara/ev_4.rpy:75
translate polish kiara_4_4b9060f0:

    # kiara "Normally you won't be able to feel this. But extraordinary pathways are like 'Its' essence."
    kiara "Normalnie nie będziesz w stanie tego odczuć. Ale niezwykłe ścieżki są jak „Jego” esencja.{#mtl}{#tl}"

# game/characters/kiara/ev_4.rpy:76
