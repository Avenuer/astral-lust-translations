# game/characters/player/ev_3.rpy:11
translate french main_3_23c718ed:

    # "You open your eyes only to see yourself standing amidst the endless expanse known as the cosmos."
    "Vous ouvrez les yeux et vous vous retrouvez au milieu de l'étendue infinie qu'est le cosmos."

# game/characters/player/ev_3.rpy:12
translate french main_3_8de9100f:

    # th "This dream again! It feels so real.."
    th "Encore ce rêve! Ça semble si réel.."

# game/characters/player/ev_3.rpy:13
translate french main_3_99b949dd_1:

    # "*whispers"
    "*chuchotements"

# game/characters/player/ev_3.rpy:16
